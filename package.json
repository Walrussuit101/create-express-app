    "dependencies": {
        "@types/fs-extra": "^9.0.13",
        "@types/node": "^17.0.10",
        "fs-extra": "^10.0.0",
        "nodemon": "^2.0.15",
        "ts-node": "^10.4.0",
        "typescript": "^4.5.5"
    },
    "devDependencies": {
        "husky": "^7.0.4",
        "lint-staged": "^12.3.3",
        "prettier": "2.5.1"
