{
    "name": "create-express-app",
    "version": "1.0.0",
    "description": "A tool to quickstart various express-y projects using templates",
    "main": "src/index.ts",
    "scripts": {
        "start": "ts-node src/index.ts",
        "dev": "nodemon -e ts,js,json --exec \"ts-node src/index.ts\"",
