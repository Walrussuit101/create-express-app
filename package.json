{
    "name": "create-express-app",
    "version": "1.0.0",
    "description": "A tool to quickstart various express-y projects using templates",
    "main": "src/index.ts",
    "scripts": {
        "start": "ts-node src/index.ts",
        "dev": "nodemon -e ts,js,json --exec \"ts-node src/index.ts\"",
        "test": "jest --verbose",
        "test:coverage": "jest --verbose --coverage",
        "compile": "tsc",
        "prepare": "husky install"
    },
    "lint-staged": {
        "**/*": "prettier --write --ignore-unknown"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/Walrussuit101/create-express-app.git"
    },
    "keywords": [],
    "author": "",
    "license": "ISC",
    "bugs": {
        "url": "https://github.com/Walrussuit101/create-express-app/issues"
    },
    "homepage": "https://github.com/Walrussuit101/create-express-app#readme",
    "dependencies": {
        "chalk": "^4.1.2",
        "commander": "^9.0.0",
        "fs-extra": "^10.0.0"
    },
    "devDependencies": {
        "@types/fs-extra": "^9.0.13",
        "@types/jest": "^27.4.0",
        "@types/node": "^17.0.21",
        "husky": "^7.0.4",
        "jest": "^27.5.1",
        "lint-staged": "^12.3.3",
        "nodemon": "^2.0.15",
        "prettier": "2.5.1",
        "ts-jest": "^27.1.3",
        "ts-node": "^10.5.0",
        "typescript": "^4.6.2"
    }
}
